{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "ScriptHost", "ESNext"],
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "target": "ESNext"
  }
}
